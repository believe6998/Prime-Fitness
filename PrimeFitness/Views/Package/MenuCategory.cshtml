@using PrimeFitness.Models.Entities;
@model List<DanhMuc>





<div class="col-3 d-none d-lg-block">
    <div class="pl-4">
        <div class="list-category">
            <h3 class="font-weight-bold text-danger pb-3 pt-5">DANH MỤC</h3>
            <div class="category-item-list pt-4 font-weight-bolder">
                @foreach (DanhMuc item in Model)
                {
                    <div class="category-item"><a href="/package/category/@item.MaDM">@item.TenDM</a></div>
                }
            </div>
        </div>
    </div>
</div>